{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "cycle-scripts-one-fits-all": "7.2.1",
    "@types/mocha": "^5.2.5",
    "cyclejs-test-helpers": "^4.0.0",
    "snabbdom-looks-like": "^1.0.5",
    "snabbdom-pragma": "^2.8.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "scripts": {
    "format": "prettier --write '{src,test}/**/*.{js,jsx,ts,tsx}'",
    "start": "cycle-scripts start",
    "start-dev": "node -r dotenv/config node_modules/cycle-scripts-one-fits-all/scripts/start.js dotenv_config_path=env/dev",
    "start-test": "node -r dotenv/config node_modules/cycle-scripts-one-fits-all/scripts/start.js dotenv_config_path=env/test",
    "test": "cycle-scripts test",
    "build": "cycle-scripts build",
    "eject": "cycle-scripts eject",
    "clean": "cycle-scripts clean"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "include": [
      "src/components"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "mocha-webpack": {
    "include": [
      "test/**/*.test.{js,jsx,ts,tsx}"
    ]
  },
  "dependencies": {
    "@cycle/dom": "^22.3.0",
    "@cycle/history": "^7.1.0",
    "@cycle/isolate": "^4.2.0",
    "@cycle/run": "^5.2.0",
    "@cycle/state": "^1.1.0",
    "@cycle/time": "^0.19.0",
    "@types/mapbox-gl": "^1.5.0",
    "@types/offscreencanvas": "^2019.6.0",
    "@types/ramda": "^0.25.00",
    "@types/request-promise": "^4.1.45",
    "@types/webgl2": "0.0.5",
    "cyclejs-utils": "^4.0.2",
    "cyclic-router": "^6.0.0",
    "dotenv": "^8.2.0",
    "json-loader": "^0.5.7",
    "mapbox-gl": "^1.5.0",
    "query-string": "^6.9.0",
    "ramda": "^0.25.0",
    "serialize-javascript": "^2.1.2",
    "switch-path": "^1.2.0",
    "three": "^0.125.0",
    "xstream": "^11.7.0"
  },
  "one-fits-all": true
}
